/************************************************************
 * MAPSETS
 *   Like aPair class but for a trio of data.
 ************************************************************/
class MapSet
{
  // Some variables
  BigInteger A;
  BigInteger B;
  BigInteger B2;
  
  // Constructor
  MapSet(BigInteger a, BigInteger b, BigInteger b2)
  {
    A = a;
    B = b;
    B2 = b2;
  }
}
